    "@types/testing-library__jest-dom": "5.14.7",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "@vitest/coverage-c8": "0.31.4",
    "clean-package": "2.2.0",
    "jsdom": "22.1.0",
    "tsup": "6.7.0",
    "typescript": "5.1.6",
    "vite": "4.4.0",
    "vitest": "0.31.4",
    "vue": "3.2.47"
